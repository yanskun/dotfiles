// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+n",
    "command": "explorer.newFile"
  },
  {
    "key": "shift+cmd+n",
    "command": "explorer.newFolder"
  },
  {
    "key": "ctrl+]",
    "command": "ghqProjectManager.openProject"
  },
  {
    "key": "ctrl+shift+l",
    "command": "versionlens.icons.hideVersionLenses",
    "when": "versionlens.providerActive && versionlens.show && !textCompareEditorVisible && !versionlens.providerBusy && !versionlens.providerError"
  },
  {
    "key": "ctrl+shift+l",
    "command": "versionlens.icons.showVersionLenses",
    "when": "versionlens.providerActive && !textCompareEditorVisible && !versionlens.providerBusy && !versionlens.providerError && !versionlens.show"
  }
]
